{
  "name": "zliq",
  "version": "1.7.1",
  "description": "slim and quick framework in low loc",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest --runInBand --watch",
    "start": "webpack-dev-server --config webpack/demo.js",
    "format": "prettier **/*.{js,scss,json} --write",
    "build": "webpack --config webpack/build.js --output-filename=zliq.js --output-path=lib/",
    "build:min": "webpack -p --config webpack/build_min.js --output-filename=zliq.min.js --output-path=lib/",
    "build:all": "npm run build && npm run build:min",
    "build:demo": "webpack --config webpack/demo.js",
    "build:lib": "npm run build && npm run build:min",
    "start:benchmark": "webpack-dev-server --config webpack/benchmark.js",
    "build:benchmark": "webpack --config webpack/benchmark.js",
    "build-prod": "npm run build:benchmark",
    "prepublishOnly": "npm test && rimraf lib/** && npm run build:lib",
    "ghpages": "npm run build:demo && (git add bundle* & git commit -m 'updated gh-pages') & git subtree push --prefix demo origin gh-pages"
  },
  "author": "Fabian <faboweber@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "deep-equal": "^1.0.1",
    "safe-eval": "^0.3.0",
    "zliq-router": "^0.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-h-jsx": "^1.0.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.24.0",
    "css-loader": "^0.26.1",
    "jest": "^23.6.0",
    "jquery": "^3.2.1",
    "materialize-css": "^0.98.2",
    "node-sass": "^4.5.0",
    "null-loader": "^0.1.1",
    "prettier": "^1.11.1",
    "prismjs": "^1.6.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-node-externals": "^1.5.4",
    "worker-loader": "^0.8.0"
  },
  "repository": {
    "url": "https://github.com/faboweb/zliq.git",
    "type": "git"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|jpg|png)$": "<rootDir>/test/helpers/empty-module.js"
    },
    "collectCoverage": true,
    "coverageDirectory": "./",
    "coverageReporters": [
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/demo/",
      "/test/",
      "/lib/"
    ],
    "testURL": "http://localhost"
  }
}
